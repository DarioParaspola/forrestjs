    "@babel/core": "^7.5.5",
    "@forrestjs/babel-preset-package": "^3.6.0",
    "@marcopeg/utils": "^2.0.1",
    "cross-env": "^5.1.4"
  },
  "homepage": "https://github.com/forrestjs/forrestjs/tree/master/packages/hooks#readme",
  "keywords": [
