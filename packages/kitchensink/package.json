{
  "name": "@forrestjs/kitchensink",
  "description": "ForrestJS demonstrational tool",
  "version": "3.21.0-alpha.3.2",
  "author": "Marco Pegoraro <marco.pegoraro@gmail.com",
  "bugs": {
    "url": "https://github.com/forrestjs/forrestjs/issues"
  },
  "main": "src/index",
  "scripts": {
    "clean": "rm -rf node_modules && rm -f yarn.lock && rm -f package-lock.json",
    "start:hooks": "cross-env NODE_ENV=development npx nodemon hooks",
    "start:service-fastify": "cross-env NODE_ENV=development npx nodemon service-fastify",
    "test:service-fastify": "jest -t service-fastify --runInBand ",
    "tdd:service-fastify": "jest -t service-fastify --runInBand  --watchAll",
    "start:service-fastify-static": "cross-env NODE_ENV=development npx nodemon service-fastify-static",
    "test:service-fastify-static": "jest -t service-fastify-static --runInBand ",
    "tdd:service-fastify-static": "jest -t service-fastify-static --runInBand  --watchAll",
    "start:service-jwt": "cross-env NODE_ENV=development npx nodemon service-jwt",
    "test:service-jwt": "jest -t service-jwt --runInBand ",
    "tdd:service-jwt": "jest -t service-jwt --runInBand  --watchAll",
    "start:service-hash": "cross-env NODE_ENV=development npx nodemon service-hash",
    "test:service-hash": "jest -t service-hash --runInBand ",
    "tdd:service-hash": "jest -t service-hash --runInBand  --watchAll",
    "start:service-fetchq": "cross-env NODE_ENV=development npx nodemon service-fetchq",
    "test:service-fetchq": "jest -t service-fetchq --runInBand ",
    "tdd:service-fetchq": "jest -t service-fetchq --runInBand  --watchAll",
    "start:app01": "cross-env NODE_ENV=development npx nodemon app01",
    "test:app01": "jest -t app01 --runInBand ",
    "tdd:app01": "jest -t app01 --runInBand  --watchAll"
  },
  "dependencies": {
    "@forrestjs/hooks": "^3.21.0-alpha.3.2",
    "@forrestjs/service-fastify": "^3.21.0-alpha.3.2",
    "@forrestjs/service-fastify-static": "^3.21.0-alpha.3.2",
    "@forrestjs/service-env": "^3.21.0-alpha.3.2",
    "@forrestjs/service-jwt": "^3.21.0-alpha.3.2",
    "@forrestjs/service-hash": "^3.21.0-alpha.3.2",
    "@forrestjs/service-fetchq": "^3.21.0-alpha.3.2"
  },
  "homepage": "https://github.com/forrestjs/forrestjs/tree/master/packages/service-fastify#readme",
  "keywords": [
    "forrestjs",
    "kitchensink"
  ],
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "repository": "https://github.com/forrestjs/forrestjs/tree/master/packages/service-fastify",
