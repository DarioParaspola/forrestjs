{
  "name": "@forrestjs/service-env",
  "description": "ForrestJS feature which helps importing environment variables from .env.* files.",
  "version": "0.0.51-alpha.0",
  "author": "Marco Pegoraro <marco.pegoraro@gmail.com",
  "bugs": {
    "url": "https://github.com/forrestjs/forrestjs/issues"
  },
  "dependencies": {
    "@babel/runtime": "^7.0.0",
    "@marcopeg/hooks": "^0.2.2",
    "node-env-file": "^0.1.8"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@forrestjs/babel-preset-package": "^0.0.51-alpha.0",
    "cross-env": "^5.1.4"
  },
  "homepage": "https://github.com/forrestjs/forrestjs/tree/master/packages/service-env#readme",
  "keywords": [
    "forrestjs",
    "service",
    "env",
    "dotenv",
    "environment"
  ],
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "repository": "https://github.com/forrestjs/forrestjs/tree/master/packages/service-env",
  "scripts": {
    "build": "babel src --out-dir . --ignore **/__tests__",
    "prepare": "cross-env NODE_ENV=production npm run build",
    "watch": "babel -w src --out-dir . --ignore **/__tests__"
  }
}
